func collectOrToggle() {
    if isOnGem {
        collectGem()
    } else if isOnClosedSwitch {
        toggleSwitch()

    }
}
for i in 1 ... 2 {
    moveForward()
    collectOrToggle()
    turnLeft()
}
for i in 1 ... 3 {
    moveForward()
    collectOrToggle()
    moveForward()
    collectOrToggle()
    turnLeft()
}
