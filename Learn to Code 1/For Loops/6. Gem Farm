func oneGemRow() {
    turnRight()
    moveForward()
    collectGem()
    moveForward()
    collectGem()
    turnLeft()
    turnLeft()
    moveForward()
    moveForward()
    turnRight()
    moveForward()
}
func oneSwitchRow() {
    turnRight()
    moveForward()
    toggleSwitch()
    moveForward()
    toggleSwitch()
    turnLeft()
    turnLeft()
    moveForward()
    moveForward()
    turnRight()
    moveForward()

}
for i in 1 ... 3 {
    oneGemRow()
}
turnLeft()
turnLeft()
moveForward()
for i in 1 ... 3 {
    oneSwitchRow()
}
