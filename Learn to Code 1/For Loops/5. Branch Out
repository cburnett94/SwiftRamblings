func turnAround() {
    turnLeft()
    turnLeft()
}
for i in 1 ... 3 {
    moveForward()
    moveForward()
    turnRight()
    moveForward()
    moveForward()
    moveForward()
    moveForward()
    moveForward()
    moveForward()
    moveForward()
    toggleSwitch()
    turnAround()
    moveForward()
    moveForward()
    moveForward()
    moveForward()
    moveForward()
    moveForward()
    moveForward()
    turnRight()
    
}
